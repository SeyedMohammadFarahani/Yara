            <gradient android:endColor="#049485" android:startColor="#049485" android:type="linear"></gradient>
            <stroke android:width="@dimen/_2sdp" android:color="#ffffff" />
        </shape>

    </item>

    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
            <gradient android:endColor="#ffffff" android:startColor="#ffffff" android:type="linear"></gradient>
            <stroke android:width="@dimen/_2sdp" android:color="#049485" />
        </shape>
    </item>


</selector>